{
  "name": "@dccs/react-datagrid-mui",
  "version": "1.0.3",
  "description": "A light datagrid build upon react-table-plain for React in a Material-UI theme.",
  "main": "lib/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "run-s lint transpile",
    "transpile": "tsc",
    "lint": "tslint ./src/**/*.ts*",
    "test": "jest --runInBand --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DCCS-IT-Business-Solutions/react-datagrid-mui.git"
  },
  "keywords": [
    "npm",
    "node",
    "react",
    "table",
    "sorting",
    "filter",
    "paging",
    "javascrip",
    "typescript"
  ],
  "author": "Stephan Mei√üner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DCCS-IT-Business-Solutions/react-datagrid-mui/issues"
  },
  "homepage": "https://github.com/DCCS-IT-Business-Solutions/react-datagrid-mui#readme",
  "dependencies": {
    "@dccs/react-table-mui": "^1.0.2",
    "@dccs/react-table-plain": "^0.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.7.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/styled-components": "^4.1.14",
    "styled-components": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "c**/?(*.)(spec|test).ts?(x)"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  }
}
